<h1>Edit Cohort</h1>

<%= form_for @cohort, url: {action: "update"} do |f| %>
  <%= render partial: "form", locals: {f: f} %>

  <%= f.hidden_field :user_type, value: @teacher.id %>
  <%= submit_tag "Submit", class: "btn btn-default" %>
  <button type="button" class="btn btn-default"><%= link_to "Back", "/teachers" %></button>
<% end %>