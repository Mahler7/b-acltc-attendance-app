<h1>Edit Profile</h1>

<%= form_for @student, url: {action: "update"} do |f| %>
  <%= render partial: "form", locals: {f: f} %>

  <%= f.hidden_field :user_type, value: "student" %>
  <%= submit_tag "Submit", class: "btn btn-default" %>
  <%= link_to "Back", "/students", class: "btn btn-default" %>
<% end %>